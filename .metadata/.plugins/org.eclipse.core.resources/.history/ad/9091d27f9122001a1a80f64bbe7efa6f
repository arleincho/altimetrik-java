package com.altimetrik.demo.dao;

public class Collage {

	private String name;
	private int id;
	private int size;

	public College(int id, String name, int size) {
		this.id = id;
		this.name = name;
		this.size = size;
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getSize() {
		return size;
	}

	public void setSize(int size) {
		this.size = size;
	}

	@Override
	public String toString() {
		return "details: " + this.name;
	}

}
