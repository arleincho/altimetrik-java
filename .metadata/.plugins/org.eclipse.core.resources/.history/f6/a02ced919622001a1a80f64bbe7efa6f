		System.out.println("pppp" + o1.getDetail().get("size"));
		
		int i = Integer.valueOf(o1.getDetail().get("size").toString());
		int j = Integer.valueOf(o1.getDetail().get("size").toString());
//		(int)o1.getDetail().get("size").
		if (i >= j) {
			return 1;
		}
		return 0;
	}

	
